# Project Configuration
PROJECT_NAME=frappeBench
COMPOSE_PROJECT_NAME=frappe-alpha

# Workspace: alpha
WORKSPACE_NAME=alpha
CODENAME=alpha
HOST_PORT=8201
NGINX_HOST_PORT=8081
INFRA_DEBUG=0

# User configuration
USER=brett
UID=1000
GID=1000

# Database configuration (uses existing frappe-mariadb container)
DB_HOST=frappe-mariadb
DB_PORT=3306
DB_PASSWORD=frappe

# Redis configuration (uses existing frappe redis containers)
REDIS_CACHE=frappe-redis-cache:6379
REDIS_QUEUE=frappe-redis-queue:6379
REDIS_SOCKETIO=frappe-redis-socketio:6379

# Frappe site configuration
SITE_NAME=alpha.localhost
ADMIN_PASSWORD=admin

# App configuration
APP_BRANCH=main

# Bench configuration
FRAPPE_BENCH_PATH=/workspace/bench
BENCH_DISABLE_UV=0

# Resource Limits
CONTAINER_MEMORY=4g
CONTAINER_CPUS=2
